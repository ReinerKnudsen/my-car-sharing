{
	"name": "carsharing-app",
	"private": true,
	"version": "2.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"prebuild": "node scripts/generate-version.js",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test": "echo 'Tests temporarily disabled'",
		"test:unit": "echo 'Unit tests temporarily disabled'",
		"test:unit:watch": "vitest",
		"test:unit:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test.e2e": "cypress run",
		"test.unit": "vitest",
		"lint": "eslint src --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
		"ios": "npm run build && npx cap sync && npx cap open ios",
		"android": "npm run build && npx cap sync && npx cap open android",
		"sync": "npx cap sync",
		"sync:ios": "npx cap sync ios",
		"sync:android": "npx cap sync android",
		"test:db": "tsx scripts/test-db-connection.ts",
		"changelog:test": "node scripts/test-changelog.js",
		"changelog:update": "node scripts/update-changelog.js"
	},
	"dependencies": {
		"@capacitor/android": "^7.4.4",
		"@capacitor/app": "7.1.0",
		"@capacitor/core": "7.4.4",
		"@capacitor/haptics": "7.0.2",
		"@capacitor/ios": "^7.4.4",
		"@capacitor/keyboard": "7.0.3",
		"@capacitor/status-bar": "7.0.3",
		"@ionic/react": "^8.5.0",
		"@ionic/react-router": "^8.5.0",
		"@supabase/supabase-js": "^2.86.0",
		"@types/react-router": "^5.1.20",
		"@types/react-router-dom": "^5.3.3",
		"ionicons": "^7.4.0",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-router": "^5.3.4",
		"react-router-dom": "^5.3.4",
		"supabase": "^2.66.0"
	},
	"devDependencies": {
		"@capacitor/cli": "7.4.4",
		"@octokit/rest": "^20.0.2",
		"@playwright/test": "^1.57.0",
		"@testing-library/dom": ">=7.21.4",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^16.2.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/dotenv": "^6.1.1",
		"@types/node": "^24.10.1",
		"@types/react": "19.0.10",
		"@types/react-dom": "19.0.4",
		"@vitejs/plugin-legacy": "^5.0.0",
		"@vitejs/plugin-react": "^4.0.1",
		"cypress": "^13.5.0",
		"dotenv": "^17.2.3",
		"eslint": "^9.20.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^15.15.0",
		"happy-dom": "^20.0.10",
		"jsdom": "^22.1.0",
		"prettier": "^3.7.4",
		"terser": "^5.4.0",
		"tsx": "^4.20.6",
		"typescript": "~5.9.0",
		"typescript-eslint": "^8.24.0",
		"vite": "^5.0.0",
		"vitest": "^0.34.6"
	},
	"description": "An Ionic project"
}
